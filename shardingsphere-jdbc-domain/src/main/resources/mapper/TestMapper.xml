<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.courage.shardingsphere.jdbc.domain.mapper.OrderMapper">

    <!-- 通用查询映射结果 -->
    <resultMap id="BaseResultMap" type="com.courage.shardingsphere.jdbc.domain.po.TEntOrder">
        <id column="id" property="id"/>
        <result column="ent_id" property="entId"/>
        <result column="region_code" property="regionCode"/>
        <result column="amount" property="amount"/>
        <result column="mobile" property="mobile"/>
        <result column="create_time" property="createTime"/>
        <result column="update_time" property="updateTime"/>
    </resultMap>

    <select id="getOrderById" parameterType="java.lang.Long" resultMap="BaseResultMap">
        select *
        FROM
        t_ent_order t
        where t.id = #{id}
    </select>

</mapper>
